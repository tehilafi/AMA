<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/a_listView_pre"
    android:layout_height="match_parent"
    android:layout_width="match_parent">


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profileuserID"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:scaleType="centerCrop"
        android:layout_marginTop="13dp"
        android:src="@drawable/profil"
        app:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019" />


    <ImageView
        android:id="@+id/starID"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:src="@drawable/star"
        app:layout_constraintBottom_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/profileuserID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/profileuserID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_creator="1"
        app:layout_constraintTop_toTopOf="@+id/profileuserID"
        app:layout_constraintVertical_bias="1.0"
        app:layout_editor_absoluteX="359dp"
        app:layout_editor_absoluteY="104dp"
        tools:layout_constraintBottom_creator="0"
        tools:layout_constraintRight_creator="0"
        tools:layout_constraintTop_creator="0" />

    <TextView
        android:id="@+id/userNameID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="userName"
        android:textColor="#615e5e"
        android:textSize="12sp"
        android:textStyle="bold"
        android:fontFamily="sans-serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        app:layout_constraintStart_toStartOf="@+id/profileuserID"
        app:layout_constraintTop_toBottomOf="@+id/profileuserID" />

    <TextView
        android:id="@+id/dateID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="4dp"
        android:fontFamily="sans-serif"
        android:text="date"
        android:textColor="#615e5e"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@+id/userNameID"
        app:layout_constraintTop_toTopOf="@+id/userNameID" />

    <TextView
        android:id="@+id/questionID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:paddingBottom="15dp"
        android:text="question"
        android:textColor="#615e5e"
        android:textSize="10sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/profileuserID"
        app:layout_constraintTop_toBottomOf="@+id/profileuserID"/>

    <ImageView
        android:id="@+id/with_ansID"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginBottom="10dp"
        android:focusable="false"
        app:layout_constraintBottom_toTopOf="@+id/questionID"
        app:layout_constraintEnd_toEndOf="@+id/questionID" />

</androidx.constraintlayout.widget.ConstraintLayout>